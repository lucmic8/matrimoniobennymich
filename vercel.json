{
  "build": {
    "env": {
      "GOOGLE_DRIVE_FOLDER_ID": "@google-drive-folder-id",
      "GOOGLE_SERVICE_ACCOUNT_KEY": "@google-service-account-key"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}